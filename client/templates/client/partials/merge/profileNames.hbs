<table>
  {{#each names}}
    <tr>
      <td>
        <div class="name" {{> signatureTooltip signatures=signatures}} {{#unless confirmed}} style="color: #8c1b13;" {{/unless}}>
          <span class="{{#if (isEqual ../index 0)}}left-profile-value{{else}}right-profile-value{{/if}}">{{first}} {{#if middle}}{{middle}} {{/if}}{{last}}</span>{{#if preferred}} <small>(Preferred)</small>{{/if}}
        </div>
      </td>
    </tr>
  {{/each}}
</table>
