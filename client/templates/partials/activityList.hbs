<ul class="list-unstyled submissions-list activity-list">
  {{#each notes}}
    <li class="note note-activity {{#if details.isDeleted}}trashed{{/if}}" data-id="{{id}}">
    {{#if version}}
      {{> noteActivityV2 options=../activityOptions}}
    {{else}}
      {{> noteActivity options=../activityOptions}}
    {{/if}}
    </li>
  {{else}}
    <li><p class="empty-message">{{activityOptions.emptyMessage}}</p></li>
  {{/each}}
</ul>
